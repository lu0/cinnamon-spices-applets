#. metadata.json->name
#. settings-schema.json->cinnamenuProvidersSection->title
msgid "Cinnamenu"
msgstr "Cinnamenu"

#. metadata.json->description
msgid ""
"A flexible menu providing formatting options, web bookmarks, open window "
"lookup, and search provider support with fuzzy searching."
msgstr ""
"Un menù flessibile che offre opzioni di formattazione, segnalibri Web, "
"ricerca a finestra aperta e supporto del provider di ricerca con ricerca "
"fuzzy."

#. settings-schema.json->panel->title
msgid "Behavior"
msgstr "Comportamento"

#. settings-schema.json->appearance->title
msgid "Appearance"
msgstr "Aspetto"

#. settings-schema.json->menu->title
msgid "Layout and Content"
msgstr "Layout e Contenuto"

#. settings-schema.json->providers->title
msgid "Search Providers"
msgstr "Provider di Ricerca"

#. settings-schema.json->extensionProvidersSection->title
msgid "No search provider extensions found"
msgstr "Nessuna estensione del provider di ricerca trovata"

#. settings-schema.json->panel-appear->title
#. settings-schema.json->panel-behave->title
msgid "Panel"
msgstr "Pannello"

#. settings-schema.json->menu-behave->title
#. settings-schema.json->menu-icons->title
msgid "Menu"
msgstr "Menù"

#. settings-schema.json->menu-layout->title
msgid "Layout"
msgstr "Layout"

#. settings-schema.json->menu-content->title
msgid "Content"
msgstr "Contenuto"

#. settings-schema.json->privacy-settings-button->description
msgid "Toggle recent files"
msgstr "Attiva/Disattiva i file recenti"

#. settings-schema.json->privacy-settings-button->tooltip
msgid "Press this button to customize your privacy settings."
msgstr ""
"Premi questo pulsante per personalizzare le tue impostazioni sulla privacy."

#. settings-schema.json->menu-editor-button->description
msgid "Open the menu editor"
msgstr "Apri l'editor del menù"

#. settings-schema.json->menu-editor-button->tooltip
msgid "Press this button to customize your menu entries."
msgstr "Premere questo pulsante per personalizzare le voci del menù."

#. settings-schema.json->resetCategoryOrder-button->description
msgid "Reset category order"
msgstr "Ripristina l'ordine delle categorie"

#. settings-schema.json->menu-icon-custom->description
msgid "Use a custom icon"
msgstr "Usa un'icona personalizzata"

#. settings-schema.json->menu-icon-custom->tooltip
msgid "Unchecking this allows the theme to set the icon"
msgstr "Deselezionando questa opzione si consente al tema di impostare l'icona"

#. settings-schema.json->menu-icon->description
msgid "Icon"
msgstr "Icona"

#. settings-schema.json->menu-icon->tooltip
msgid "Select an icon to show in the panel."
msgstr "Seleziona un'icona da mostrare nel pannello."

#. settings-schema.json->menu-label->description
#. settings-schema.json->menu-label->tooltip
msgid "Set the custom menu button icon name"
msgstr "Imposta il nome dell'icona sul pulsante del menù personalizzato"

#. settings-schema.json->category-icon-size->units
#. settings-schema.json->apps-list-icon-size->units
#. settings-schema.json->apps-grid-icon-size->units
msgid "pixels"
msgstr "pixels"

#. settings-schema.json->category-icon-size->description
#. settings-schema.json->category-icon-size->tooltip
msgid "Category icon size"
msgstr "Dimensione dell'icona della categoria"

#. settings-schema.json->apps-list-icon-size->description
#. settings-schema.json->apps-list-icon-size->tooltip
msgid "Applications list icon size"
msgstr "Dimensione icona elenco applicazioni"

#. settings-schema.json->apps-grid-icon-size->description
#. settings-schema.json->apps-grid-icon-size->tooltip
msgid "Applications grid icon size"
msgstr "Dimensione dell'icona della griglia delle applicazioni"

#. settings-schema.json->overlay-key->description
msgid "Keyboard shortcut to open and close the menu"
msgstr "Scorciatoia da tastiera per aprire e chiudere il menù"

#. settings-schema.json->activate-on-hover->description
msgid "Open the menu when I move my mouse over it"
msgstr "Apri il menù al passaggio del mouse"

#. settings-schema.json->activate-on-hover->tooltip
msgid "Enable opening the menu when the mouse enters the applet"
msgstr "Abilita l'apertura del menù quando il mouse entra nell'applet"

#. settings-schema.json->hover-delay->units
#. settings-schema.json->tooltip-delay->units
msgid "milliseconds"
msgstr "millisecondi"

#. settings-schema.json->hover-delay->description
msgid "Menu hover delay:"
msgstr "Ritardo passaggio sul menù:"

#. settings-schema.json->hover-delay->tooltip
msgid "Delay between switching categories"
msgstr "Ritardo tra il cambio di categorie"

#. settings-schema.json->enable-animation->description
msgid "Use menu animations"
msgstr "Usa animazioni del menù"

#. settings-schema.json->enable-animation->tooltip
msgid "Allow the menu to animate on open and close"
msgstr "Consenti al menù di animarsi all'apertura e alla chiusura"

#. settings-schema.json->category-click->description
msgid "Activate categories on click"
msgstr "Attiva le categorie al clic"

#. settings-schema.json->category-click->tooltip
msgid ""
"When enabled, category views are activated on click instead of on hover."
msgstr ""
"Se abilitato, le viste delle categorie vengono attivate al clic anziché al "
"passaggio del mouse."

#. settings-schema.json->enable-autoscroll->description
msgid "Enable autoscrolling in application list"
msgstr "Abilita scorrimento automatico nell'elenco delle applicazioni"

#. settings-schema.json->enable-autoscroll->tooltip
msgid ""
"Choose whether or not to enable smooth autoscrolling in the application list."
msgstr ""
"Scegli se abilitare o meno lo scorrimento automatico nell'elenco delle "
"applicazioni."

#. settings-schema.json->enable-bookmarks->description
msgid "Show web bookmarks"
msgstr "Mostra segnalibri Web"

#. settings-schema.json->enable-bookmarks->tooltip
msgid "Choose whether or not bookmarks are indexed in the menu."
msgstr "Scegli se i segnalibri devono essere indicizzati nel menù."

#. settings-schema.json->enable-windows->description
msgid "Enable searching of open windows"
msgstr "Abilita la ricerca di finestre aperte"

#. settings-schema.json->enable-windows->tooltip
msgid "Choose whether or not open windows are indexed in menu searches."
msgstr ""
"Scegli se le finestre aperte vengono indicizzate o meno nelle ricerche nei "
"menù."

#. settings-schema.json->enable-search-providers->description
msgid "Enable search providers"
msgstr "Abilita i provider di ricerca"

#. settings-schema.json->enable-search-providers->tooltip
msgid ""
"Choose whether or not search provider results are indexed in menu searches."
msgstr ""
"Scegliere se indicizzare o meno i risultati del provider di ricerca nelle "
"ricerche di menù."

#. settings-schema.json->get-providers->description
msgid "Example Search Providers"
msgstr "Provider di Ricerca di Esempio"

#. settings-schema.json->startup-view-mode->description
#. settings-schema.json->startup-view-mode->tooltip
msgid "Startup applications view mode"
msgstr "Modalità di visualizzazione delle applicazioni di avvio"

#. settings-schema.json->startup-view-mode->options
msgid "List"
msgstr "Elenco"

#. settings-schema.json->startup-view-mode->options
msgid "Grid"
msgstr "Griglia"

#. settings-schema.json->apps-grid-column-count->units
msgid "columns"
msgstr "colonne"

#. settings-schema.json->apps-grid-column-count->description
#. settings-schema.json->apps-grid-column-count->tooltip
msgid "Number of columns in apps grid"
msgstr "Numero di colonne nella griglia delle app"

#. settings-schema.json->show-places->description
msgid "Show bookmarks and places"
msgstr "Mostra segnalibri e luoghi"

#. settings-schema.json->show-places->tooltip
msgid "Choose whether or not to show bookmarks and places in the menu."
msgstr "Scegli se mostrare o meno i segnalibri e i luoghi nel menù."

#. settings-schema.json->show-application-icons->description
msgid "Show application icons"
msgstr "Mostra le icone delle applicazioni"

#. settings-schema.json->show-application-icons->tooltip
msgid "Choose whether or not to show icons on applications."
msgstr "Scegli se mostrare o meno le icone sulle applicazioni."

#. settings-schema.json->show-category-icons->description
msgid "Show category icons"
msgstr "Mostra le icone di categoria"

#. settings-schema.json->show-category-icons->tooltip
msgid "Choose whether or not to show icons on categories."
msgstr "Scegli se mostrare o meno le icone sulle categorie."

#. settings-schema.json->search-filesystem->description
msgid "Enable filesystem path entry in search box"
msgstr ""
"Abilita l'immissione del percorso del filesystem nella casella di ricerca"

#. settings-schema.json->search-filesystem->tooltip
msgid "Allows path entry in the menu search box."
msgstr "Consente l'inserimento del percorso nella casella di ricerca del menù."

#. settings-schema.json->show-apps-description-on-buttons->description
msgid "Show application descriptions under titles"
msgstr "Mostra le descrizioni delle applicazioni sotto i titoli"

#. settings-schema.json->show-apps-description-on-buttons->tooltip
msgid ""
"Choose whether or not to show the application description on the application "
"buttons."
msgstr ""
"Scegliere se mostrare o meno la descrizione dell'applicazione sui pulsanti "
"dell'applicazione."

#. settings-schema.json->show-tooltips->description
msgid "Show tooltips"
msgstr "Mostra suggerimenti"

#. settings-schema.json->show-tooltips->tooltip
msgid ""
"When enabled, tooltips containing the item's name or description is shown "
"when hovered over."
msgstr ""
"Se abilitato, vengono visualizzati i suggerimenti contenenti il nome o la "
"descrizione dell'oggetto quando si passa il mouse sopra."

#. settings-schema.json->tooltip-delay->description
msgid "Tooltip delay"
msgstr "Ritardo dei suggerimenti"

#. settings-schema.json->tooltip-delay->tooltip
msgid ""
"The delay time in milliseconds for tooltips when hovering over a menu item."
msgstr ""
"Il tempo di ritardo in millisecondi per i suggerimenti quando si passa con "
"il mouse su una voce di menù."

#. settings-schema.json->enable-custom-menu-height->description
#. settings-schema.json->enable-custom-menu-height->tooltip
msgid "Use a custom menu height"
msgstr "Usa un'altezza del menù personalizzata"

#. settings-schema.json->custom-menu-height->units
msgid "px"
msgstr "px"

#. settings-schema.json->custom-menu-height->description
#. settings-schema.json->custom-menu-height->tooltip
msgid "Custom menu height"
msgstr "Altezza menù personalizzata"
